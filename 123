text = input()
if text == 'кашель':
    tip = input()
    if tip == 'влажный':
        print('Назначаю вам Отхаркин-1')
    else:
        print('Назначаю вам Отхаркин-2')
elif text == 'температура':
    lvl = float(input())
    if float(lvl) >= float(38.0):
        print('Антижарин-1')
    else:
        print('Антижарин-2')
else:
 print('Пожалуйста, обратитесь к доктору Айболиту')
